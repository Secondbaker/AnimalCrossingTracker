<% value = collectible_attribute.collectible_attribute_value%>
<% value_class = value.class %>

<% if (value_class == BellValue) then %>
  <%= render partial: "bell_values/show", locals: { bell_value: value } %>
<% elsif (value_class == TimeOfDay) then %>
  <%= render partial: "time_of_days/show", locals: { time_of_day: value } %>
<% elsif (value_class == TimeOfYear) then %>
  <%= render partial: "time_of_years/show", locals: { time_of_year: value } %>
<% elsif value_class == FishingLocation %>
  <%= render partial: "fishing_locations/show", locals: { fishing_location: value } %>
<% elsif value_class == BugLocation %>
  <%= render partial: "bug_locations/show", locals: { bug_location: value } %>
<% elsif value_class == ShadowSize %>
  <%= render partial: "shadow_sizes/show", locals: { shadow_size: value } %>
<% elsif value_class == VillagerGender %>
  <%= render partial: "villager_genders/show", locals: { villager_gender: value } %>
<% elsif value_class == Birthday %>
  <%= render partial: "birthdays/show", locals: { birthday: value } %>
<% elsif value_class == StringCollectibleAttribute %>
  <%= render partial: "string_collectible_attributes/show", locals: { string_collectible_attribute: value } %>
<% elsif value_class == CollectibleAttributeList %>
  <%= render partial: "collectible_attribute_lists/show", locals: { collectible_attribute_list: value } %>
<% elsif value_class == NumberCollectibleAttribute %>
  <%= render partial: "number_collectible_attributes/show", locals: { number_collectible_attribute: value } %>
<% end %>