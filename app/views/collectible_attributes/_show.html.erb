<% if (collectible_attribute.collectible_attribute_value.class == Rarity) then %>
  <%= render partial: "rarities/show", locals: { rarity: collectible_attribute.collectible_attribute_value } %>
<% elsif (collectible_attribute.collectible_attribute_value.class == BellValue) then %>
  <%= render partial: "bell_values/show", locals: { bell_value: collectible_attribute.collectible_attribute_value } %>
<% elsif (collectible_attribute.collectible_attribute_value.class == TimeOfDay) then %>
  <%= render partial: "time_of_days/show", locals: { time_of_day: collectible_attribute.collectible_attribute_value } %>
<% elsif (collectible_attribute.collectible_attribute_value.class == TimeOfYear) then %>
  <%= render partial: "time_of_years/show", locals: { time_of_year: collectible_attribute.collectible_attribute_value } %>
<% elsif collectible_attribute.collectible_attribute_value.class == FishingLocation %>
  <%= render partial: "fishing_locations/show", locals: { fishing_location: collectible_attribute.collectible_attribute_value } %>
<% elsif collectible_attribute.collectible_attribute_value.class == BugLocation %>
  <%= render partial: "bug_locations/show", locals: { bug_location: collectible_attribute.collectible_attribute_value } %>
<% elsif collectible_attribute.collectible_attribute_value.class == ShadowSize %>
  <%= render partial: "shadow_sizes/show", locals: { shadow_size: collectible_attribute.collectible_attribute_value } %>
<% end %>