<p id="notice"><%= notice %></p>

<h1>Reward Title Positions</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @reward_title_positions.each do |reward_title_position| %>
      <tr>
        <td><%= link_to 'Show', reward_title_position %></td>
        <td><%= link_to 'Edit', edit_reward_title_position_path(reward_title_position) %></td>
        <td><%= link_to 'Destroy', reward_title_position, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Reward Title Position', new_reward_title_position_path %>
