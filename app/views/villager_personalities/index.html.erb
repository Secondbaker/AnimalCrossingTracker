<p id="notice"><%= notice %></p>

<h1>Villager Personalities</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @villager_personalities.each do |villager_personality| %>
      <tr>
        <td><%= link_to 'Show', villager_personality %></td>
        <td><%= link_to 'Edit', edit_villager_personality_path(villager_personality) %></td>
        <td><%= link_to 'Destroy', villager_personality, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Villager Personality', new_villager_personality_path %>
