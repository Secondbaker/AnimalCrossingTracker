<p id="notice"><%= notice %></p>

<h1>Bug Locations</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bug_locations.each do |bug_location| %>
      <tr>
        <td><%= link_to 'Show', bug_location %></td>
        <td><%= link_to 'Edit', edit_bug_location_path(bug_location) %></td>
        <td><%= link_to 'Destroy', bug_location, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bug Location', new_bug_location_path %>
