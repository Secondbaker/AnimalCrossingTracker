<p id="notice"><%= notice %></p>

<h1>Collectible Attribute Types</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @collectible_attribute_types.each do |collectible_attribute_type| %>
      <tr>
        <td><%= collectible_attribute_type.name %></td>
        <td><%= link_to 'Show', collectible_attribute_type %></td>
        <td><%= link_to 'Edit', edit_collectible_attribute_type_path(collectible_attribute_type) %></td>
        <td><%= link_to 'Destroy', collectible_attribute_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Collectible Attribute Type', new_collectible_attribute_type_path %>
